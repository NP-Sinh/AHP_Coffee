{
    "version": 2,
    "builds": [
      {
        "src": "AHP_COFFEE/src/QueryImg.py",
        "use": "@vercel/python"
      },
      {
        "src": "AHP_COFFEE/src/images/**",
        "use": "@vercel/static"
      },
      {
        "src": "AHP_COFFEE/src/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "AHP_COFFEE/src/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "AHP_COFFEE/src/QueryImg.py"
      }
    ]
  }
  